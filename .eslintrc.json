{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "camelcase": "off"
    },
    "globals": {
        "axios": false,
        "route": false,
        "moment": false,
        "APP_VERSION": false
    }
}
